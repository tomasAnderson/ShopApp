<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:view="clr-namespace:ShopApp.MyView"
                    xmlns:vm="clr-namespace:ShopApp.MyViewModel">
    <DataTemplate DataType="{x:Type vm:UCSalesViewModel}">
        <view:UCSales />
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:UCChargesViewModel}">
        <view:UCCharges />
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:UCCurrentMonthIncomeViewModel}">
        <view:UCCurrentMonthIncome />
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:UCTop5ChargesViewModel}">
        <view:UCTop5Charges />
    </DataTemplate>
</ResourceDictionary>